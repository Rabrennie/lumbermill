{
  "name": "@rabrennie/lumbermill",
  "version": "0.2.2",
  "description": "Ezpz management of git worktrees",
  "author": "rabrennie",
  "bin": {
    "lm": "./bin/run",
    "lumbermill": "./bin/run"
  },
  "homepage": "https://github.com/rabrennie/lumbermill",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/rabrennie/lumbermill",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2"
  },
  "devDependencies": {
    "@oclif/test": "^2.4.4",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.40",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.11.3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "lm",
    "binAliases": [
      "lumbermill"
    ],
    "dirname": "lumbermill",
    "commands": "./dist/commands",
    "plugins": [],
    "topicSeparator": " ",
    "topics": {},
    "hooks": {
      "init": [
        "./dist/hooks/init/check-git-command",
        "./dist/hooks/init/check-first-run"
      ],
      "command_not_found": "./dist/hooks/command_not_found/run-alias-command"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/rabrennie/lumbermill/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
